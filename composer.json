{
    "name": "lokus/dto",
    "version": "1.0.1",
    "description": "Package for creating DTOs",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Lokus\\Dto\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^12",
        "symfony/var-dumper": "^7.3",
        "phpstan/phpstan": "^2.1"
    },
    "scripts": {
        "test": "phpunit",
        "lint": "phpstan",
        "verify": [
            "@test",
            "@lint"
        ]
    }
}
